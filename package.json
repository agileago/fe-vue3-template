{
  "name": "fe-vue3-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vue-cli-service serve",
    "uat": "vue-cli-service build --mode uat",
    "release": "vue-cli-service build",
    "preview": "serve -s dist",
    "type:check": "tsc --noEmit --skipLibCheck",
    "format": "prettier --write '**/*.{ts,tsx,js,vue,css,scss,html,json}'",
    "lint": "eslint --fix .",
    "api": "tsg -c ./src/api/tsg.config.ts",
    "commit": "cz-customizable",
    "chrome": "open -n /Applications/Google\\ Chrome.app/ --args --disable-web-security  --user-data-dir=$HOME/MyChromeDevUserData/"
  },
  "dependencies": {
    "@abraham/reflection": "^0.9.1",
    "ant-design-vue": "^3.0.0-beta.13",
    "axios": "0.21.1",
    "core-js": "^3.21.1",
    "injection-js": "^2.4.0",
    "path-to-regexp": "^6.2.0",
    "ts-deepmerge": "^2.0.1",
    "ts-essentials": "^9.1.2",
    "tslib": "^2.3.1",
    "vue": "^3.2.31",
    "vue-router": "^4.0.14",
    "vue3-oop": "^0.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.17.7",
    "@commitlint/cli": "^16.2.1",
    "@nexhome/yorkie": "^2.0.8",
    "@types/swagger-schema-official": "^2.0.22",
    "@types/webpack-env": "^1.16.3",
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "@typescript-eslint/parser": "^5.15.0",
    "@vue/cli-plugin-babel": "~5.0.3",
    "@vue/cli-plugin-typescript": "~5.0.3",
    "@vue/cli-service": "~5.0.3",
    "@vue3-oop/babel-plugin-jsx": "^1.1.1",
    "@vue3-oop/jsx-hot-loader": "^0.1.6",
    "autoprefixer": "^10.4.3",
    "babel-loader": "^8.2.3",
    "babel-plugin-import": "^1.13.3",
    "co": "4",
    "commitizen": "^4.2.4",
    "commitlint-config-cz": "^0.13.3",
    "cz-customizable": "^6.3.0",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "less": "^4.1.2",
    "less-loader": "^10.2.0",
    "lint-staged": "^12.3.5",
    "postcss": "^8.4.11",
    "prettier": "^2.4.1",
    "sass": "^1.49.9",
    "sass-loader": "^12.6.0",
    "serve": "^13.0.2",
    "tailwindcss": "^3.0.23",
    "ts-gear": "^4.6.1",
    "typescript": "~4.6.2",
    "vite": "^2.8.6",
    "vue-cli-plugin-mock": "^1.0.3",
    "webpack": "^5.70.0",
    "webpack-aliyun-oss": "^0.5.2"
  },
  "gitHooks": {
    "pre-commit": "lint-staged",
    "commit-msg": "commitlint -E GIT_PARAMS"
  },
  "lint-staged": {
    "*.{js,jsx,vue,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{css,scss,html,json}": [
      "prettier --write"
    ]
  }
}
