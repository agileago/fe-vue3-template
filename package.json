{
  "name": "fe-vue3-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vue-cli-service serve",
    "uat": "vue-cli-service build --mode uat",
    "release": "vue-cli-service build",
    "preview": "serve -s dist",
    "type:check": "tsc --noEmit --skipLibCheck",
    "format": "prettier --write '**/*.{ts,tsx,js,vue,css,scss,html,json}'",
    "lint": "eslint --fix .",
    "api": "tsg -c ./src/api/tsg.config.ts",
    "chrome": "open -n /Applications/Google\\ Chrome.app/ --args --disable-web-security  --user-data-dir=$HOME/MyChromeDevUserData/"
  },
  "dependencies": {
    "@abraham/reflection": "^0.10.0",
    "ant-design-vue": "^3.2.2",
    "axios": "0.27.2",
    "core-js": "^3.22.4",
    "injection-js": "^2.4.0",
    "path-to-regexp": "^6.2.0",
    "ts-deepmerge": "^2.0.1",
    "ts-essentials": "^9.1.2",
    "tslib": "^2.4.0",
    "vue": "^3.2.33",
    "vue-router": "^4.0.15",
    "vue3-oop": "^0.5.8"
  },
  "devDependencies": {
    "@babel/core": "^7.17.10",
    "@commitlint/cli": "^16.2.4",
    "@commitlint/config-conventional": "^16.2.1",
    "@nexhome/yorkie": "^2.0.8",
    "@types/swagger-schema-official": "^2.0.22",
    "@types/webpack-env": "^1.16.4",
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/parser": "^5.22.0",
    "@vue/cli-plugin-babel": "~5.0.4",
    "@vue/cli-plugin-typescript": "~5.0.4",
    "@vue/cli-service": "~5.0.4",
    "@vue3-oop/jsx-hot-loader": "^0.1.6",
    "autoprefixer": "^10.4.7",
    "babel-loader": "^8.2.5",
    "babel-plugin-import": "^1.13.5",
    "co": "4",
    "commitizen": "^4.2.4",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "less": "^4.1.2",
    "less-loader": "^10.2.0",
    "lint-staged": "^12.4.1",
    "postcss": "^8.4.13",
    "prettier": "^2.6.2",
    "sass": "^1.51.0",
    "sass-loader": "^12.6.0",
    "serve": "^13.0.2",
    "tailwindcss": "^3.0.24",
    "ts-gear": "^4.7.3",
    "tsconfig-paths-webpack-plugin": "^3.5.2",
    "typescript": "~4.6.4",
    "vite": "^2.9.8",
    "vue-cli-plugin-mock": "^1.0.3",
    "webpack": "^5.72.0",
    "webpack-aliyun-oss": "^0.5.6"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "gitHooks": {
    "pre-commit": "lint-staged",
    "commit-msg": "commitlint -E GIT_PARAMS"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{js,css,scss,html,json}": [
      "prettier --write"
    ]
  }
}
