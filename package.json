{
  "name": "fe-vue3-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vue-cli-service serve",
    "build": "vue-cli-service build --no-module",
    "develop": "pnpm run build --mode develop",
    "preview": "serve -s dist",
    "type:check": "tsc --noEmit",
    "format": "prettier --write '**/*.{ts,tsx,js,vue,css,scss,html,json}'",
    "lint": "eslint --fix .",
    "api": "tsg -c ./src/api/tsg.config.ts",
    "chrome": "open -n /Applications/Google\\ Chrome.app/ --args --disable-web-security  --user-data-dir=$HOME/MyChromeDevUserData/",
    "postinstall": "simple-git-hooks"
  },
  "dependencies": {
    "@abraham/reflection": "^0.10.0",
    "ant-design-vue": "^3.2.12",
    "axios": "0.27.2",
    "core-js": "^3.25.0",
    "injection-js": "^2.4.0",
    "path-to-regexp": "^6.2.1",
    "ts-essentials": "^9.3.0",
    "tslib": "^2.4.0",
    "vue": "^3.2.38",
    "vue-router": "^4.1.5",
    "vue3-oop": "^1.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.18.13",
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@types/swagger-schema-official": "^2.0.22",
    "@types/webpack-env": "^1.18.0",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.36.1",
    "@vue/cli-plugin-babel": "~5.0.8",
    "@vue/cli-plugin-typescript": "~5.0.8",
    "@vue/cli-service": "~5.0.8",
    "@vue3-oop/jsx-hot-loader": "^0.1.6",
    "babel-loader": "^8.2.5",
    "babel-plugin-import": "^1.13.5",
    "eslint": "~8.22.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "less": "^4.1.3",
    "less-loader": "^11.0.0",
    "lint-staged": "^13.0.3",
    "postcss": "^8.4.16",
    "postcss-preset-env": "^7.8.0",
    "prettier": "^2.7.1",
    "sass": "^1.54.8",
    "sass-loader": "^13.0.2",
    "serve": "^14.0.1",
    "simple-git-hooks": "^2.8.0",
    "tailwindcss": "^3.1.8",
    "ts-gear": "^4.11.2",
    "tsconfig-paths-webpack-plugin": "^4.0.0",
    "typescript": "~4.8.2",
    "vite": "^3.0.9",
    "vue-cli-plugin-mock": "^1.0.3",
    "webpack": "^5.74.0",
    "webpack-aliyun-oss": "^0.5.8"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm exec lint-staged",
    "commit-msg": "pnpm exec commitlint -e \"$@\""
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{js,css,scss,html,json}": [
      "prettier --write"
    ]
  }
}
